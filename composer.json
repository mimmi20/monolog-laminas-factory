{
  "name": "mimmi20/monolog-laminas-factory",
  "type": "library",
  "description": "Provides a Factory to build a Laminas Logger using Monolog.",
  "homepage": "https://github.com/mimmi20/monolog-laminas-factory",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "laminas/laminas-log": "^2.13.1",
    "monolog/monolog": "^2.3.1"
  },
  "require-dev": {
    "aws/aws-sdk-php": "^3.185.16",
    "cmdisp/monolog-microsoft-teams": "^1.2.0",
    "doctrine/couchdb": "1.0.0-beta4",
    "graylog2/gelf-php": "^1.7.0",
    "infection/infection": "^0.23.0",
    "jk/monolog-request-header-processor": "^1.0.0",
    "laminas/laminas-config": "^3.5.0",
    "laminas/laminas-dependency-plugin": "^2.1.2",
    "laminas/laminas-modulemanager": "^2.10.2",
    "mikey179/vfsstream": "^1.6.9",
    "mimmi20/coding-standard": "^2.2.3",
    "pepakriz/phpstan-exception-rules": "^0.11.7",
    "php-console/php-console": "^3.1.8",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.92",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.21",
    "phpunit/phpunit": "^9.5.7",
    "predis/predis": "^1.1.7",
    "rollbar/rollbar": "^2.1.0 || ^3.0.0",
    "ruflin/elastica": "^7.1.1",
    "swiftmailer/swiftmailer": "^6.2.7"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mimmi20\\LoggerFactory\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\LoggerFactory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LoggerFactory\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/monolog-laminas-factory/issues",
    "source": "https://github.com/mimmi20/monolog-laminas-factory"
  }
}
